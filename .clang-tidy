Checks:
  - "*"
  - "google-explicit-constructor"
  - "-abseil-*"
  - "-altera-*"
  - "-android-*"
  - "-boost-*"
  - "-darwin-*"
  - "-fuchsia-*"
  - "-google-*"
  - "-linuxkernel-*"
  - "-llvm-*"
  - "-llvmlibc-*"
  - "-objc-*"
  - "-openmp-*"
  - "-zircon-*"
  - "-clang-diagnostic-c++98*"
  - "-clang-diagnostic-pre-c++14*"
  - "-clang-diagnostic-pre-c++17*"
  - "-clang-diagnostic-pre-c++20*"
  - "-clang-diagnostic-padded"
